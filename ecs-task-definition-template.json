{
  "family": "haemeok-task",
  "networkMode": "bridge",
  "executionRoleArn": "arn:aws:iam::595712054986:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "haemeok-app",
      "image": "595712054986.dkr.ecr.ap-northeast-2.amazonaws.com/haemeok-repository:latest",
      "essential": true,
      "portMappings": [
        {
          "name": "haemeok-app-8080-tcp",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "cookies-volume",
          "containerPath": "/home/ec2-user/cookies",
          "readOnly": true
        }
      ],
      "environment": [
        { "name": "DB_URL", "value": "${DB_URL}" },
        { "name": "DB_USERNAME", "value": "${DB_USERNAME}" },
        { "name": "DB_PASSWORD", "value": "${DB_PASSWORD}" },
        { "name": "JWT_SECRET", "value": "${JWT_SECRET}" },
        { "name": "ACCESS_TOKEN_VALIDITY", "value": "${ACCESS_TOKEN_VALIDITY}" },
        { "name": "REFRESH_TOKEN_VALIDITY", "value": "${REFRESH_TOKEN_VALIDITY}" },
        { "name": "GOOGLE_CLIENT_ID", "value": "${GOOGLE_CLIENT_ID}" },
        { "name": "GOOGLE_CLIENT_SECRET", "value": "${GOOGLE_CLIENT_SECRET}" },
        { "name": "KAKAO_CLIENT_ID", "value": "${KAKAO_CLIENT_ID}" },
        { "name": "KAKAO_CLIENT_SECRET", "value": "${KAKAO_CLIENT_SECRET}" },
        { "name": "NAVER_CLIENT_ID", "value": "${NAVER_CLIENT_ID}" },
        { "name": "NAVER_CLIENT_SECRET", "value": "${NAVER_CLIENT_SECRET}" },
        { "name": "CLOUD_AWS_REGION_STATIC", "value": "${CLOUD_AWS_REGION_STATIC}" },
        { "name": "CLOUD_AWS_S3_BUCKET", "value": "${CLOUD_AWS_S3_BUCKET}" },
        { "name": "AWS_ACCESS_KEY_ID", "value": "${AWS_ACCESS_KEY_ID}" },
        { "name": "AWS_SECRET_ACCESS_KEY", "value": "${AWS_SECRET_ACCESS_KEY}" },
        { "name": "APP_S3_BUCKET_NAME", "value": "${APP_S3_BUCKET_NAME}" },
        { "name": "APP_S3_UPLOAD_BASE_PATH", "value": "${APP_S3_UPLOAD_BASE_PATH}" },
        { "name": "APP_S3_PRESIGNED_URL_EXPIRATION_MINUTES", "value": "${APP_S3_PRESIGNED_URL_EXPIRATION_MINUTES}" },
        { "name": "OPENSEARCH_HOST", "value": "${OPENSEARCH_HOST}" },
        { "name": "REPLICATE_TOKEN", "value": "${REPLICATE_TOKEN}" },
        { "name": "OPENAI_API_KEY", "value": "${OPENAI_API_KEY}" },
        { "name": "REDIS_PASSWORD", "value": "${REDIS_PASSWORD}" },
        { "name": "GEMINI_API_KEY", "value": "${GEMINI_API_KEY}" },
        { "name": "GROK_API_KEY", "value": "${GROK_API_KEY}" },
        { "name": "APP_HASHIDS_SALT", "value": "${APP_HASHIDS_SALT}" },
        { "name": "APP_HASHIDS_MIN_LENGTH", "value": "${APP_HASHIDS_MIN_LENGTH}" },
        { "name": "APP_YTDLP_TMP_BASE_DIR", "value": "/tmp" },
        { "name": "YTDLP_PROXY_URL", "value": "${YTDLP_PROXY_URL}" },
        { "name": "GEMINI_STUDIO_API_KEY", "value": "${GEMINI_STUDIO_API_KEY}" },
        { "name": "YOUTUBE_API_KEYS", "value": "${YOUTUBE_API_KEYS}" },
        { "name": "APPLE_TEAM_ID", "value": "${APPLE_TEAM_ID}" },
        { "name": "APPLE_KEY_ID", "value": "${APPLE_KEY_ID}" },
        { "name": "APPLE_PRIVATE_KEY", "value": "${APPLE_PRIVATE_KEY}" },
        { "name": "APPLE_CLIENT_ID", "value": "${APPLE_CLIENT_ID}" },
        { "name": "GEMINI_CREDENTIALS", "value": "${GEMINI_CREDENTIALS}" },
        { "name": "APP_TEST_LOGIN_ENABLED", "value": "${APP_TEST_LOGIN_ENABLED}" },
        { "name": "COUPANG_ACCESS_KEY", "value": "${COUPANG_ACCESS_KEY}" },
        { "name": "COUPANG_SECRET_KEY", "value": "${COUPANG_SECRET_KEY}" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/haemeok-task",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "volumes": [
    {
      "name": "cookies-volume",
      "host": {
        "sourcePath": "/home/ec2-user/cookies"
      }
    }
  ],
  "requiresCompatibilities": ["EC2"],
  "memory": "1536",
  "cpu": "1024"
}
